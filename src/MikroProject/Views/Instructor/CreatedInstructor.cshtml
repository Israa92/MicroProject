@model InstructorViewModel
<h1>Created Instructor</h1>


    <table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Course Name</th>
    </tr>
    @foreach (var item in Instructor)
    {
        <tr>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Course</td>
        </tr>

    }
</table>

       